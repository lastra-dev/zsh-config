ZSH_HIGHLIGHT_STYLES[global-alias]='fg=#8bcd5b'
ZSH_HIGHLIGHT_STYLES[function]='fg=#8bcd5b'
ZSH_HIGHLIGHT_STYLES[command]='fg=#8bcd5b'
ZSH_HIGHLIGHT_STYLES[precommand]='fg=#8bcd5b,italic'
ZSH_HIGHLIGHT_STYLES[autodirectory]='fg=#efbd5d,italic'
ZSH_HIGHLIGHT_STYLES[single-hyphen-option]='fg=#efbd5d'
ZSH_HIGHLIGHT_STYLES[double-hyphen-option]='fg=#efbd5d'
ZSH_HIGHLIGHT_STYLES[back-quoted-argument]='fg=#c75ae8'
## Keywords
## Built ins
